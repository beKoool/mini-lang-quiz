{
  "expo": {
    "name": "Mini Language Quiz",
    "slug": "mini-lang-quiz",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "minilangquiz",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "splash": {
      "backgroundColor": "#5e5cf1",
      "resizeMode": "contain"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#5e5cf1"
      },
      "package": "com.anujsapkota.minilangquiz",
      "versionCode": 1,
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],

      [
        "expo-font",
      {
        "fonts": ["node_modules/@expo-google-fonts/inter/400Regular/Inter_400Regular.ttf"]
      } 
      ]   
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
